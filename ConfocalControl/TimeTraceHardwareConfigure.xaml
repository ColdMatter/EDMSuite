<Window x:Class="ConfocalControl.TimeTraceHardwareConfigure"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TimeTraceHardwareConfigure" Height="240" Width="300">
    <Grid>
        <Button x:Name="ok_button" Content="OK" HorizontalAlignment="Left" Margin="72,154,0,0" VerticalAlignment="Top" Width="75" Click="ok_button_Click"/>
        <Button x:Name="cancel_button" Content="Cancel" HorizontalAlignment="Left" Margin="152,154,0,0" VerticalAlignment="Top" Width="75" Click="cancel_button_Click"/>
        <ComboBox x:Name="output_type_box" HorizontalAlignment="Left" Margin="90,52,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="output_type_box_SelectionChanged"/>
        <ComboBox x:Name="output_box" HorizontalAlignment="Left" Margin="90,79,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
