Cell[BoxData[
 RowBox[{
  RowBox[{"getBlockFile", "[", 
   RowBox[{"clusterName_", ",", "clusterIndex_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "files", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"files", "=", 
      RowBox[{
       RowBox[{
