      RowBox[{"\"\<Jul\>\"", "->", "\"\<July\>\""}], ",", "\n", 
      RowBox[{"\"\<Aug\>\"", "->", "\"\<August\>\""}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<Sep\>\"", "->", "\"\<September\>\""}], ",", 
