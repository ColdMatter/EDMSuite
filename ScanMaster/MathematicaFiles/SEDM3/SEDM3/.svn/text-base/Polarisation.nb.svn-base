Cell["\<\
Calculate the polarisation curve and store it in the Applications directory\
\>", "Section"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"calculateAndCachePolarisation", "[", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "ME", ",", "SubH", ",", "H", ",", "HSmall", ",", "HSmallNum", ",", "ev",
        ",", "evs", ",", "levels", ",", "groundState", ",", "\[Alpha]"}], 
      "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Off", "[", 
       RowBox[{"ClebschGordan", "::", "phy"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Off", "[", 
       RowBox[{"ClebschGordan", "::", "tri"}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ME", "[", " ", 
        RowBox[{"J_", ",", "j_", ",", "K_", ",", "k_"}], "]"}], ":=", " ", 
       RowBox[{
        RowBox[{
         FractionBox[
          SuperscriptBox["\[HBar]", "2"], 
          RowBox[{"2", "A"}]], "J", 
         RowBox[{"(", 
          RowBox[{"J", "+", "1"}], ")"}], 
         RowBox[{"KroneckerDelta", "[", 
          RowBox[{"J", ",", "K"}], "]"}], 
         RowBox[{"KroneckerDelta", "[", 
          RowBox[{"j", ",", "k"}], "]"}]}], "+", 
        RowBox[{"\[Mu]", "  ", "F", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}], 
          RowBox[{
           RowBox[{"2", "J"}], "-", "j"}]], 
         SqrtBox[
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "J"}], "+", "1"}], ")"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "K"}], "+", "1"}], ")"}]}]], 
         RowBox[{"ThreeJSymbol", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"J", ",", 
             RowBox[{"-", "j"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"K", ",", "k"}], "}"}]}], "]"}], 
         RowBox[{"ThreeJSymbol", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"J", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"K", ",", "0"}], "}"}]}], "]"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"SubH", "[", 
        RowBox[{"J_", ",", "K_"}], "]"}], ":=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"J", "-", "K"}], "]"}], "\[GreaterEqual]", "2"}], ",", 
         RowBox[{"Table", "[", " ", 
          RowBox[{"0", ",", " ", 
           RowBox[{"{", 
            RowBox[{"j", ",", 
             RowBox[{"-", "J"}], ",", "J"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", 
             RowBox[{"-", "K"}], ",", "K"}], "}"}]}], "]"}], ",", 
         RowBox[{"Table", "[", " ", 
          RowBox[{
           RowBox[{"ME", "[", 
            RowBox[{"J", ",", "j", ",", "K", ",", "k"}], "]"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"j", ",", 
             RowBox[{"-", "J"}], ",", "J"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", 
             RowBox[{"-", "K"}], ",", "K"}], "}"}]}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"H", "[", "N_", "]"}], ":=", 
       RowBox[{"ArrayFlatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"SubH", "[", 
           RowBox[{"i", ",", "j"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", "N"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "0", ",", "N"}], "}"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"HSmall", "=", 
       RowBox[{"H", "[", "7", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"HSmallNum", "[", "f_", "]"}], ":=", 
       RowBox[{"N", "[", 
        RowBox[{"HSmall", "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"F", "\[Rule]", "f"}], ",", " ", 
           RowBox[{"\[Mu]", "\[Rule]", "1"}], ",", 
           RowBox[{"A", "\[Rule]", "0.5"}], ",", 
           RowBox[{"\[HBar]", "\[Rule]", "1"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Energy", "[", "f_", "]"}], ":=", 
       RowBox[{"Eigenvalues", "[", 
        RowBox[{"HSmallNum", "[", "f", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ev", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Energy", "[", "f", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"f", ",", "0", ",", "20", ",", "0.1"}], "}"}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"evs", "=", 
       RowBox[{"Transpose", "[", 
        RowBox[{"Sort", "/@", "ev"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"levels", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"0.1", 
              RowBox[{"(", 
               RowBox[{"i", "-", "1"}], ")"}]}], ",", 
             RowBox[{
              RowBox[{
              "evs", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], 
              "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], "}"}],
            ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", 
             RowBox[{"Length", "[", 
              RowBox[{
              "evs", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], 
              "]"}]}], "}"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "1", ",", 
           RowBox[{"Length", "[", "evs", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"groundState", "=", 
       RowBox[{"ListInterpolation", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Transpose", "[", 
           RowBox[{
           "levels", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
           "]"}], "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], ",",
          
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Transpose", "[", 
            RowBox[{
            "levels", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
            "]"}], "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
          "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"SEDM3`Polarisation`\[Alpha]", "[", "\[Lambda]_", "]"}], "=", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"groundState", "[", "y", "]"}], ",", "y"}], "]"}]}], "/.", 
        
        RowBox[{"y", "\[Rule]", "\[Lambda]"}]}]}], ";", "\[IndentingNewLine]",
       
      RowBox[{"SetDirectory", "[", 
       RowBox[{
       "$TopDirectory", "<>", "\"\<\\\\AddOns\\\\Applications\\\\\>\""}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"DumpSave", "[", 
       RowBox[{"\"\<polSEDM3.mx\>\"", ",", "SEDM3`Polarisation`\[Alpha]"}], 
       "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"On", "[", 
       RowBox[{"ClebschGordan", "::", "phy"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"On", "[", 
       RowBox[{"ClebschGordan", "::", "tri"}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.394346257981*^9, 3.394346261151*^9}, {3.394346501814*^9,
